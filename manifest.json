{
    "name": "Lavalink",
    "id": "lavalink",
    "packaging_format": 1,
    "description": {
        "en": "Standalone audio sending node based on Lavaplayer.",
        "fr": "Nœud d'envoi audio autonome basé sur Lavaplayer."
    },
    "version": "1.0~ynh1",
    "url": "https://example.com",
    "upstream": {
        "license": "MIT License",
        "website": "https://example.com",
        "demo": "https://demo.example.com",
        "admindoc": "https://yunohost.org/packaging_apps",
        "userdoc": "https://yunohost.org/apps",
        "code": "https://github.com/freyacodes/Lavalink"
    },
    "license": "free",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 11.0.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.4-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "password",
                "type": "password",
                "help": {
                    "en": "Use the help field to add an information for the admin about this question.",
                    "fr": "Utilisez le champ aide pour ajouter une information à l'intention de l'administrateur à propos de cette question."
                }
            }
        ]
    }
}
